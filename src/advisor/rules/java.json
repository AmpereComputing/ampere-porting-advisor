{
    "languageRules": {
        "name": "Java",
        "detailsUrl": "https://amperecomputing.com/tuning-guides/unlocking-java-performance-tuning-guide",
        "extensions": [".java", ".jar", ".war"],
        "dependencyFiles": ["pom.xml"],
        "minVersion": "8",
        "recommendedVersion": "11"
    },
    "toolsRules": [
        {
            "name": "Eclipse Temurin",
            "details": "detected java code. we recommend using Eclipse Temurin version 17 or above release for aarch64.",
            "detailsUrl": "https://adoptium.net/temurin/releases"
        }
    ],
    "libraryRules": [
        { "name": "commons-crypto", "minVersion": "1.1.0" },
        { "name": "Fingagle", "minVersion": "20.12.0" },
        { "name": "hadoop-lzo", "specialInstructions": "this library requires a manual build", "detailsUrl": "https://github.com/twitter/hadoop-lzo#building-and-configuring" },
        { "name": "jansi-native", "specialInstructions": "this library requires a manual build", "detailsUrl": "https://github.com/fusesource/jansi-native#building" },
        { "name": "jffi", "minVersion": "1.2.13" },
        { "name": "leveldbjni-all", "unsupported": true },
        { "name": "lz4-java", "minVersion": "1.4.0" },
        { "name": "netty-transport-native-epoll", "minVersion": "4.1.50" },
        { "name": "netty-tcname", "minVersion": "2.0.31" },
        { "name": "rockdbjni", "minVersion": "5.0.1" },
        { "name": "sigar", "specialInstructions": "this library requires a manual build", "detailsUrl": "https://github.com/hyperic/sigar/wiki/source" },
        { "name": "snappy-java", "minVersion": "1.1.4" },
        { "name": "zstd-jni", "minVersion": "1.2.0" }
    ]
}
